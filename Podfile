platform :ios, '12.0'

target 'STYLYST' do
  use_frameworks!

  # Pods for STYLYST
  
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'Firebase/Analytics'
  pod 'Firebase/Storage'
  pod 'GeoFire', :git => 'https://github.com/firebase/geofire-objc.git', :branch => 'master'
  pod 'Geofirestore', :git => 'https://github.com/imperiumlabs/GeoFirestore-iOS.git'
  pod 'ImageSlideshow', '~> 1.9.0'
  pod 'StretchScrollView', '~> 5.0'
  

end
